{
  "name": "poke-frontend",
  "version": "0.0.1",
  "description": "Poke frontend",
  "scripts": {
    "build": "webpack",
    "build:server:dev": "cross-env NODE_ENV=development npm run build -- --env development --config ./webpack.config.server.babel.js --colors --display-error-details",
    "build:server:prod": "cross-env NODE_ENV=production npm run build -- --env production --config ./webpack.config.server.babel.js --colors --display-error-details",
    "build:client:prod": "cross-env NODE_ENV=production npm run build -- --env production --config ./webpack.config.client.babel.js",
    "build:prod": "cross-env npm run build:server:prod && cross-env npm run build:client:prod",
    "watch:client": "cross-env NODE_ENV=development node -r dotenv/config bin/webpack-dev-server.js",
    "test:dev": "cross-env NODE_ENV=development nodemon -r dotenv/config bin/server.js",
    "dev": "cross-env concurrently --kill-others \"cross-env npm run watch:client\" \"cross-env npm run build:server:dev\" \"cross-env NODE_ENV=development nodemon -r dotenv/config bin/server.js\"",
    "start": "cross-env NODE_ENV=production node -r dotenv/config bin/server.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "author": "Bryan Huang <huangchiheng@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "clean-webpack-plugin": "^2.0.2",
    "concurrently": "^4.1.2",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.2",
    "postcss-flexibility": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.4.1",
    "url-loader": "^1.1.2",
    "webpack-bundle-analyzer": "^3.5.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.4.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "cross-env": "^5.2.1",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "^0.8.0",
    "query-string": "^6.8.3",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.9.0",
    "react-icons": "^3.7.0",
    "react-paginate": "^6.3.0",
    "react-redux": "^7.1.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-rwd": "^0.1.3",
    "react-slick": "^0.24.0",
